`line 65 "oc8051_b_register.v" 0
module oc8051_b_register (clk, rst, bit_in, data_in, wr, wr_bit,
              wr_addr, data_out);


`line 69 "oc8051_b_register.v" 0
input clk, rst, wr, wr_bit, bit_in;
input [7:0] wr_addr, data_in;

`line 72 "oc8051_b_register.v" 0
output [7:0] data_out;

`line 74 "oc8051_b_register.v" 0
reg [7:0] data_out;


`line 77 "oc8051_b_register.v" 0
 
 
always @(posedge clk or posedge rst)
begin
  if (rst)
    data_out <= #1 8'h00;
  else if (wr) begin
    if (!wr_bit) begin
      if (wr_addr==8'hf0)
        data_out <= #1 data_in;
    end else begin
      if (wr_addr[7:3]==5'b11110)
        data_out[wr_addr[2:0]] <= #1 bit_in;
    end
  end
end

`line 94 "oc8051_b_register.v" 0
endmodule

